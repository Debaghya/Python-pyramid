{% extends "mysite:site/templates/base.jinja2" %}
{% block title %}{{ context.title }}{% endblock %}
{% block content %}
    <h1>{{ context.title }}</h1>
    <ul>
        <li>
            <strong>Id</strong>: {{ context.id }}
        </li>
        <li>
            <strong>username</strong>: {{ context.username }}
        </li>
        <li>
            <strong>first_name</strong>: {{ context.first_name }}
        </li>
        <li>
            <strong>last_name</strong>: {{ context.last_name }}
        </li>


    </ul>
   {% if  context.username == "Superman"  %}
        <img src="https://i.pinimg.com/originals/23/10/34/23103471342bb28c274f6f58996c87e1.jpg" alt ="super"  width=200px height =200px>

    {% elif  context.username == "Batman"  %}
         <img src="https://images.bauerhosting.com/legacy/empire-images/articles/57f27fc60c6437272f5ffc3b/ben%20affleck%20batman%20suit.jpg?format=jpg&quality=80&width=960&height=540&ratio=16-9&resize=aspectfill"  alt= "bat" width=200px height =200px>
    {% elif  context.username == "WonderWoman"  %}
        <img src="https://cdn.vox-cdn.com/thumbor/Ia_Cyth1PDyN8nLltrMSFv85rwY=/0x0:1280x640/1200x800/filters:focal(467x58:671x262)/cdn.vox-cdn.com/uploads/chorus_image/image/55115281/wonderwoman2.0.jpg" alt="wonder"  width=200px height =200px >
     {% elif  context.username == "Flash"  %}
        <img src="https://hollywoodlife.com/wp-content/uploads/2022/08/the-flash-movie-Everett-embed-1.jpg "  alt= "flash" width=200px height=200px >
      {% elif  context.username == "Green Lantern"  %}
        <img src=" data:image/jpeg;base64," alt="green"  width=200px height=200px>
       {% elif  context.username == "Aquaman"  %}
        <img src="https://cdn.britannica.com/90/199490-050-DA632B69/promotional-art-Jason-Momoa-Justic-League-2017.jpg" alt="aqua"  width=200px height=200px>
       {% elif  context.username == "Shazam"  %}
        <img src="https://i.pinimg.com/236x/b4/c7/da/b4c7daad07c444dd4fdaf021d615288e.jpg" alt="shazam"  width=200px height=200px>
       {% elif  context.username == "Cyborg"  %}
         <img src="https://i.pinimg.com/550x/4b/e2/96/4be2967b3ad5890e2eff437d323b9354.jpg" alt="cyborg"  width=200px height=200px>
       {% elif  context.username == "Martian Manhunter"  %}
         <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSDOJEC-xZ1Z_qRa_UNTW1JjeffuvYuy81Zdv4WlVQTzwprP6sgrIsR1sUpzDJb7RztBl8&usqp=CAU " alt="martian" width=200px height=200px>
     {% else %}
         {{ super() }}
    {% endif %}
{% endblock %}
